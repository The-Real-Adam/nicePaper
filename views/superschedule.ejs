<h3><%= title %></h3>

<table id="superSchedule">
  <tr>
    <th>account.id</th>
    <th>email</th>
    <th>firstName1</th>
    <th>lastName1</th>
    <th>firstName2</th>
    <th>lastName2</th>
    <th>weddingDate</th>
    <th>account_id</th>
    <th>role</th>
    <th>template_id</th>
    <th>created_at</th>
    <th>updated_at</th>
  </tr>

  <% for (let i = 0; i < data.length; i++) { %>
    <tr>

        <td><%= data[i].id %></td>
        <td><%= data[i].email %></td>
        <td><%= data[i].first_name_1 %></td>
        <td><%= data[i].last_name_1 %></td>
        <td><%= data[i].first_name_2 %></td>
        <td><%= data[i].last_name_2 %></td>
        <td><%= data[i].wedding_date %></td>
        <td><%= data[i].account_id %></td>
        <td><%= data[i].role %></td>
        <td><%= data[i].template_id %></td>
        <td><%= data[i].created_at %></td>
        <td><%= data[i].updated_at %></td>

        <td><a href="/schedule/<%= data[i].id %>/edit"><button class="edit" type="button" name="button" id="<%= data[i].id %>">edit</button></a>
        <button class="delete" type="button" name="button" id="<%= data[i].id %>">delete</button>
        <a href=""><button class="userSched" type="button" name="button" id="<%= data[i].id %> ">Get this user's schedule</button></a>
      </td>
    </tr>
  <% } %>
</table>
