<h1><%= title %></h1>

<h2><%= wedDate %></h2>

<table>
  <tr>
    <th>time</th>
    <th>event</th>
    <th>description</th>
  </tr>

  <% for (let i = 0; i < data.length; i++) { %>
    <tr>
      <td><%= data[i].time %></td>
      <td><%= data[i].item %></td>
      <td><%= data[i].description %></td>
    </tr>
  <% } %>
</table>
